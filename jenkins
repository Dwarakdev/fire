Pipeline {
  agent {
    node {
      label 'JenkinsBuildNode'
}
}
 stages {
  stage('Code Checkout') {
    steps {
     checkout([
        $class: 'GitSCM',
        branches: [[name: 'main']],
        userRemoteConfigs: [[credentialsId: 'Arshu1997!',url: '']]])
    }
  }
  stage('shell command') {
    steps {
   sh "ls -al" 
  }
  }
  }
}
